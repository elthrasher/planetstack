{
  "name": "planetstack",
  "version": "1.0.0",
  "description": "API Gateway Websocket Demo",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/elthrasher/planetstack"
  },
  "scripts": {
    "build": "npm run build:web",
    "build:web": "ts-node esbuild.ts build",
    "clean": "rimraf cdk.out coverage web/js",
    "deploy": "npm run clean && cdk deploy --require-approval never --outputs-file config.json",
    "destroy": "cdk destroy --force",
    "diff": "cdk diff",
    "lint": "eslint . --ext=.js,.ts",
    "pretest": "npm run lint",
    "start": "npm run clean && ts-node esbuild.ts",
    "synth": "cdk synth --no-staging > template.yaml",
    "test": "jest --coverage --silent",
    "test:heap": "node --expose-gc node_modules/.bin/jest --runInBand --logHeapUsage --detectOpenHandles --silent --coverage --no-cache"
  },
  "keywords": [
    "API",
    "Gateway",
    "Websocket",
    "CDK",
    "esbuild",
    "React"
  ],
  "author": "Matt Morgan",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/aws-lambda": "^8.10.87",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.18.0",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "aws-cdk": "^2.1.0",
    "esbuild": "^0.14.2",
    "esbuild-plugin-sass": "^0.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.4.4",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.1.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.1.0-alpha.0",
    "aws-cdk-lib": "^2.1.0",
    "aws-sdk": "^2.1046.0",
    "constructs": "^10.0.12",
    "dynamodb-toolbox": "^0.3.5",
    "ksuid": "^3.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-draggable": "^4.4.4",
    "react-use-websocket": "^2.9.1"
  }
}
