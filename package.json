{
  "name": "planetstack",
  "version": "1.0.0",
  "description": "API Gateway Websocket Demo",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/elthrasher/planetstack"
  },
  "scripts": {
    "build": "npm run build:web",
    "build:web": "ts-node esbuild.ts build",
    "clean": "rimraf cdk.out coverage web/js",
    "deploy": "npm run clean && cdk deploy --require-approval never --outputs-file config.json",
    "destroy": "cdk destroy --force",
    "diff": "cdk diff",
    "lint": "eslint . --ext=.js,.ts",
    "pretest": "npm run lint",
    "start": "npm run clean && ts-node esbuild.ts",
    "synth": "cdk synth --no-staging > template.yaml",
    "test": "jest --coverage --silent",
    "test:heap": "node --expose-gc node_modules/.bin/jest --runInBand --logHeapUsage --detectOpenHandles --silent --coverage --no-cache"
  },
  "keywords": [
    "API",
    "Gateway",
    "Websocket",
    "CDK",
    "esbuild",
    "React"
  ],
  "author": "Matt Morgan",
  "license": "MIT",
  "devDependencies": {
    "@aws-cdk/assert": "1.115.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/aws-lambda": "^8.10.80",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.5",
    "@types/react-dom": "^17.0.9",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "aws-cdk": "1.115.0",
    "esbuild": "^0.12.15",
    "esbuild-plugin-sass": "^0.5.2",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.3.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.115.0",
    "@aws-cdk/aws-apigatewayv2": "1.115.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "1.115.0",
    "@aws-cdk/aws-cloudfront": "1.115.0",
    "@aws-cdk/aws-cloudfront-origins": "1.115.0",
    "@aws-cdk/aws-dynamodb": "1.115.0",
    "@aws-cdk/aws-iam": "1.115.0",
    "@aws-cdk/aws-lambda": "1.115.0",
    "@aws-cdk/aws-lambda-nodejs": "1.115.0",
    "@aws-cdk/aws-logs": "1.115.0",
    "@aws-cdk/aws-s3": "1.115.0",
    "@aws-cdk/aws-s3-deployment": "1.115.0",
    "@aws-cdk/core": "1.115.0",
    "@aws-cdk/custom-resources": "1.115.0",
    "aws-sdk": "^2.953.0",
    "dynamodb-toolbox": "^0.3.4",
    "ksuid": "^2.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-draggable": "^4.4.3",
    "react-use-websocket": "^2.7.1"
  }
}
