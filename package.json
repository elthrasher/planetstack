{
  "name": "planetstack",
  "version": "1.0.0",
  "description": "API Gateway Websocket Demo",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run build:web",
    "build:web": "ts-node esbuild.ts build",
    "clean": "rimraf cdk.out coverage web/js",
    "deploy": "npm run clean && cdk deploy",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "lint": "eslint . --ext=.js,.ts",
    "pretest": "npm run lint",
    "start": "npm run clean && ts-node esbuild.ts",
    "synth": "cdk synth",
    "test": "jest --coverage"
  },
  "keywords": [
    "API",
    "Gateway",
    "Websocket",
    "CDK",
    "esbuild",
    "React"
  ],
  "author": "Matt Morgan",
  "license": "MIT",
  "devDependencies": {
    "@aws-cdk/assert": "1.106.1",
    "@types/fs-extra": "^9.0.7",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.31",
    "@types/react-dom": "^17.0.1",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "aws-cdk": "1.106.1",
    "esbuild": "^0.12.5",
    "esbuild-plugin-sass": "^0.5.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.3.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.0.3",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudfront": "1.106.1",
    "@aws-cdk/aws-cloudfront-origins": "1.106.1",
    "@aws-cdk/aws-s3": "1.106.1",
    "@aws-cdk/aws-s3-deployment": "1.106.1",
    "@aws-cdk/core": "1.106.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-draggable": "^4.4.3"
  }
}
